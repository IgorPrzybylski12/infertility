<div id="<%= dom_id protein %>" class="border border-teal-400 p-6 m-10 text-center rounded-lg shadow-md hover:shadow-lg p-6">
  <p class="my-5">
    <strong class="block font-medium mb-1">Name:</strong>
    <%= protein.name %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Description:</strong>
    <%= protein.description %>
  </p>

    <%= link_to "Show this protein", protein, class: "rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
    <%= link_to "Edit this protein", edit_protein_path(protein), class: "rounded-lg py-3 ml-2 px-5 bg-gray-100 inline-block font-medium" %>
    <hr class="mt-6">
</div>
